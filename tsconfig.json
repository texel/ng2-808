{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitHelpers": false,
    "sourceMap": true
  },
  "files": [
    "./src/app/app.spec.ts",
    "./src/app/app.ts",
    "./src/app/components/controls/controls.component.ts",
    "./src/app/components/index.ts",
    "./src/app/components/sequence-selector/sequence-selector.component.ts",
    "./src/app/components/sequencer/sequencer.component.ts",
    "./src/app/directives/router-active.ts",
    "./src/app/home/directives/x-large.spec.ts",
    "./src/app/home/directives/x-large.ts",
    "./src/app/home/home.spec.ts",
    "./src/app/home/home.ts",
    "./src/app/home/providers/title.spec.ts",
    "./src/app/home/providers/title.ts",
    "./src/app/ng-808.component.ts",
    "./src/app/services/sequencer.service.ts",
    "./src/bootstrap-sass.config.ts",
    "./src/custom_typings.d.ts",
    "./src/main.ts",
    "./src/vendor.ts",
    "./test/injector.spec.ts",
    "./test/sanity-test.spec.ts",
    "typings/main.d.ts",
    "typings/jquery/jquery.d.ts",
    "typings/lodash/lodash.d.ts"
  ],
  "filesGlob": [
    "./src/**/*.ts",
    "./test/**/*.ts",
    "!./node_modules/**/*.ts",
    "typings/main.d.ts",
    "typings/jquery/**",
    "typings/lodash/**"
  ],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": true
  }
}
